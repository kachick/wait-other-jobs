{
  "typescript": {
    "quoteStyle": "preferSingle",
    "useBraces": "always",
    "module.sortImportDeclarations": "maintain",
    "module.sortExportDeclarations": "maintain",
    "importDeclaration.sortNamedImports": "maintain",
    "exportDeclaration.sortNamedExports": "maintain"
  },
  "json": {},
  "markdown": {},
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  },
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,md,toml,yml}"],
  "excludes": ["**/node_modules", "**/*-lock.json", "lib/**", "dist/**"],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.85.1.wasm",
    "https://plugins.dprint.dev/json-0.17.4.wasm",
    "https://plugins.dprint.dev/markdown-0.15.3.wasm",
    "https://plugins.dprint.dev/toml-0.5.4.wasm",
    "https://plugins.dprint.dev/prettier-0.26.1.json@fdbe31f6aecd24f9d6b924214710a6766050d03146163b4e241e6056b2462f2e"
  ]
}
