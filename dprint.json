{
  "typescript": {
    "quoteStyle": "preferSingle",
    "useBraces": "always",
    "module.sortImportDeclarations": "maintain",
    "module.sortExportDeclarations": "maintain",
    "importDeclaration.sortNamedImports": "maintain",
    "exportDeclaration.sortNamedExports": "maintain"
  },
  "json": {},
  "markdown": {},
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  },
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,md,yml}"],
  "excludes": ["**/node_modules", "**/*-lock.json", "lib/**", "dist/**"],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.79.0.wasm",
    "https://plugins.dprint.dev/json-0.17.0.wasm",
    "https://plugins.dprint.dev/markdown-0.15.1.wasm",
    "https://plugins.dprint.dev/prettier-0.12.0.json@f8c34316196cf4d0fb2f51b19073331258e15e71a934ee04a7b3328db684c98f"
  ]
}
