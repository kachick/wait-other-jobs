{
  "typescript": {
    "quoteStyle": "preferSingle",
    "useBraces": "always",
    "module.sortImportDeclarations": "maintain",
    "module.sortExportDeclarations": "maintain",
    "importDeclaration.sortNamedImports": "maintain",
    "exportDeclaration.sortNamedExports": "maintain"
  },
  "json": {},
  "markdown": {},
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  },
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,md,yml}"],
  "excludes": ["**/node_modules", "**/*-lock.json", "lib/**", "dist/**"],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.74.0.wasm",
    "https://plugins.dprint.dev/json-0.15.6.wasm",
    "https://plugins.dprint.dev/markdown-0.14.1.wasm",
    "https://plugins.dprint.dev/prettier-0.9.0.json@76c20538ea4231b3daa9b62059f90ec144200cab5477b7e95b730cbb3ed48957"
  ]
}
